{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "CommonJS",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "strict": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "moduleResolution": "Node",
    "typeRoots": ["./node_modules/@types"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
